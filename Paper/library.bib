@article{Smith2014,
abstract = {Jones and Dzhafarov (2014) claim that several current models of speeded decision making in cognitive tasks, including the diffusion model, can be viewed as special cases of other general models or model classes. The general models can be made to match any set of response time (RT) distribution and accuracy data exactly by a suitable choice of parameters and so are unfalsifiable. The implication of their claim is that models like the diffusion model are empirically testable only by artificially restricting them to exclude unfalsifiable instances of the general model. We show that Jones and Dzhafarov's argument depends on enlarging the class of “diffusion” models to include models in which there is little or no diffusion. The unfalsifiable models are deterministic or near-deterministic growth models, from which the effects of within-trial variability have been removed or in which they are constrained to be negligible. These models attribute most or all of the variability in RT and accuracy to across-trial variability in the rate of evidence growth, which is permitted to be distributed arbitrarily and to vary freely across experimental conditions. In contrast, in the standard diffusion model, within-trial variability in evidence is the primary determinant of variability in RT. Across-trial variability, which determines the relative speed of correct responses and errors, is theoretically and empirically constrained. Jones and Dzhafarov's attempt to include the diffusion model in a class of models that also includes deterministic growth models misrepresents and trivializes it and conveys a misleading picture of cognitive decision-making research. (PsycINFO Database Record (c) 2014 APA, all rights reserved). (journal abstract)},
author = {Smith, Philip L and Ratcliff, Roger and McKoon, Gail},
doi = {10.1037/a0037667},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Smith, Ratcliff, McKoon/2014/Smith, Ratcliff, McKoon{\_}2014{\_}The diffusion model is not a deterministic growth model Comment on Jones and Dzhafarov (2014).pdf:pdf},
isbn = {6314442508},
issn = {1939-1471},
journal = {Psychological Review},
keywords = {assume that decisions are,based on,choice,currently,decision,diffusion,from processing a stimulus,making in cognitive tasks,response time,sequential sampling,successful models of speeded,the cumulative evidence obtained,two-choice decision},
month = {oct},
number = {4},
pages = {679--688},
pmid = {25347314},
publisher = {NIH Public Access},
title = {{The diffusion model is not a deterministic growth model: Comment on Jones and Dzhafarov (2014).}},
