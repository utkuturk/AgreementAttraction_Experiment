issn = {1069-9384},
journal = {Psychonomic bulletin {\&} review},
keywords = {Bayes Theorem,Humans,Models,Psychological,Psychology,Psychology: methods,Psychology: statistics {\&} numerical data},
month = {oct},
number = {5},
pages = {779--804},
pmid = {18087943},
title = {{A practical solution to the pervasive problems of p values.}},
