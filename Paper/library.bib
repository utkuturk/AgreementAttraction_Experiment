journal = {Acta Psychologica},
keywords = {Cognitive psychometrics,Decision Making,Diffusion model,Hierarchical models,Humans,Models,Psychological,Response times,Semantic categorization,Semantics,Time Factors},
month = {mar},
number = {3},
pages = {269--282},
pmid = {19962683},
publisher = {Elsevier B.V.},
title = {{A crossed random effects diffusion model for speeded semantic categorization decisions}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19962683},
volume = {133},
year = {2010}
}
