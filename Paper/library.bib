journal = {Journal of Experimental Psychology: General},
number = {5},
pages = {1837--1860},
pmid = {25000446},
title = {{Selective influence of working memory load on exceptionally slow reaction times}},
url = {http://psycnet.apa.org/journals/xge/143/5/1837.html{\%}5Cnhttp://psycnet.apa.org/journals/xge/143/5/1837.pdf},
volume = {143},
