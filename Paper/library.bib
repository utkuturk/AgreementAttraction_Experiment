@article{Tauber2016,
abstract = {Recent debates in the psychological literature have raised questions about what assumptions underpin Bayesian models of cognition, and what inferences they license about human cognition. In this paper we revisit this topic, arguing that there are two qualitatively different ways in which a Bayesian model could be constructed. If a Bayesian model is intended to license a claim about optimality then the priors and likelihoods in the model must be constrained by reference to some external criterion. A descriptive Bayesian model need not correspond to any claim that the underlying cognition is optimal or rational, and is used solely as a tool for instantiating a substantive psychological theory. We present three case studies in which these two perspectives lead to different computational models and license different conclusions about human cognition. We argue that the descriptive Bayesian approach is more useful overall, especially when combined with principled tools for model evaluation and model selection. More generally we argue for the importance of making a clear distinction between the two perspectives. Considerable confusion results when descriptive models and optimal models are conflated, and if Bayesians are to avoid contributing to this confusion it is important to avoid making normative claims when none are intended.},
author = {Tauber, Sean and Navarro, Daniel J. and Perfors, Amy and Steyvers, Mark},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Tauber et al/2016/Tauber et al.{\_}2016{\_}Bayesian models of cognition revisited Setting optimality aside and letting data drive psychological theory.pdf:pdf},
number = {November 2016},
title = {{Bayesian models of cognition revisited : Setting optimality aside and letting data drive psychological theory}},
year = {2016}
}
