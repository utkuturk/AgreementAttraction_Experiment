pages = {175--219},
pmid = {1582172},
title = {{The reviewing of object files: Object-specific integration of information}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/1582172 http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed{\&}cmd=Retrieve{\&}dopt=AbstractPlus{\&}list{\_}uids=16067246954817902351related:D9dlPPpT-t4J},
