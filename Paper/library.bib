@article{Schlesewsky2000,
abstract = {The processing of locally ambiguous wh-phrases has received much attention$\backslash$nin the lastyears, and experimental results reported in the literature$\backslash$nconverge in at least one respect:if a clause-initial NP is locally$\backslash$nambiguous between a subject and an object interpretation,the human$\backslash$nparser strongly prefers the former reading. The present chapter constitutes$\backslash$nnoexception to this general picture. Our focus therefore does$\backslash$nnot so much lie in the identification of surprising new data,but$\backslash$nrather on establishing that the subject preference is an ubiquituous$\backslash$nphenomenon inGerman, and in particular on showing that it is caused$\backslash$nby a syntax-related aspect ofsentence processing (and not, say, by$\backslash$ntuning to frequencies, by semantic preferences,etc.). Another issue$\backslash$nis the identification of the point of time in on-line processing$\backslash$natwhich the subject preference is established. A series of four$\backslash$nself-paced reading and two offline studies investigating various$\backslash$ntypesof German wh-questions will be presented, which strongly support$\backslash$na syntacticinterpretation of the subject preference. We will discuss$\backslash$nthese results in the context of abroader framework covering the processing$\backslash$nof unambiguous questions as well (cf.Schlesewsky, Fanselow, and Kliegl$\backslash$n1997a), which takes differences in memory load to bethe crucial factor$\backslash$nin the processing of wh-questions.},
author = {Schlesewsky, Matthias and Fanselow, Gisbert and Kliegl, Reinhold and Krems, Josef},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Schlesewsky et al/2000/Schlesewsky et al.{\_}2000{\_}The subject preference in the processing of locally ambiguous WH-Questions in German.pdf:pdf},
journal = {German Sentence Processing},
pages = {65--93},
title = {{The subject preference in the processing of locally ambiguous WH-Questions in German}},
year = {2000}
}
