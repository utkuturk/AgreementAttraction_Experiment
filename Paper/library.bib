@article{Wurm2014,
abstract = {Psycholinguists are making increasing use of regression analyses and mixed-effects modeling. In an attempt to deal with concerns about collinearity, a number of researchers orthogonalize predictor variables by residualizing (i.e., by regressing one predictor onto another, and using the residuals as a stand-in for the original predictor). In the current study, the effects of residualizing predictor variables are demonstrated and discussed using ordinary least-squares regression and mixed-effects models. Some of these effects are almost certainly not what the researcher intended and are probably highly undesirable. Most importantly, what residualizing does not do is change the result for the residualized variable, which many researchers probably will find surprising. Further, some analyses with residualized variables cannot be meaningfully interpreted. Hence, residualizing is not a useful remedy for collinearity. ?? 2013 Elsevier Inc.},
author = {Wurm, Lee H. and Fisicaro, Sebastiano A.},
doi = {10.1016/j.jml.2013.12.003},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Wurm, Fisicaro/2014/Wurm, Fisicaro{\_}2014{\_}What residualizing predictors in regression analyses does (and what it does not do).pdf:pdf},
isbn = {0749-596X},
issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {Collinearity,Mixed-effects modeling,Orthogonalization,Regression analysis,Residualization,Statistical analysis},
number = {1},
pages = {37--48},
publisher = {Elsevier Inc.},
title = {{What residualizing predictors in regression analyses does (and what it does not do)}},
url = {http://dx.doi.org/10.1016/j.jml.2013.12.003},
volume = {72},
year = {2014}
}
@article{Berkum2007a,
abstract = {The electrophysiology of language comprehension has long been dominated by research on syntactic and semantic integration. However, to understand expressions like "he did it" or "the little girl", combining word meanings in accordance with semantic and syntactic constraints is not enough-readers and listeners also need to work out what or who is being referred to. We review our event-related brain potential research on the processes involved in establishing reference, and present a new experiment in which we examine when and how the implicit causality associated with specific interpersonal verbs affects the interpretation of a referentially ambiguous pronoun. The evidence suggests that upon encountering a singular noun or pronoun, readers and listeners immediately inspect their situation model for a suitable discourse entity, such that they can discriminate between having too many, too few, or exactly the right number of referents within at most half a second. Furthermore, our implicit causality findings indicate that a fragment like "David praised Linda because..." can immediately foreground a particular referent, to the extent that a subsequent "he" is at least initially construed as a syntactic error. In all, our brain potential findings suggest that referential processing is highly incremental, and not necessarily contingent upon the syntax. In addition, they demonstrate that we can use ERPs to relatively selectively keep track of how readers and listeners establish reference.},
annote = {abstract:
- leser und h{\{}{\"{o}}{\}}rer inspizieren ihre situation models unmittelbar, wenn sie ein nomen oder pronomen lesen oder h{\{}{\"{o}}{\}}ren
- mit bias inkongruentes pronomen wird zun{\{}{\"{a}}{\}}chst als syntaktischer fehler verarbeitet


paper:
- bekannteste und robusteste erp-effekte: p600 (syntaktische verletzungen) und n400 (semantische verletzungen)
- sustained frontal negative shift (nref) bei schwierigkeiten mit referenzherstellung
- van berkum et al. (1999a): kontexte mit mehreren und nur einem m{\{}{\"{o}}{\}}glichen referenten {\textgreater} einfache manipulation f{\{}{\"{u}}{\}}hrt zu klarem erp-effekt (nfref bei etwa 300ms)
- zwei implikationen: (1) schnelle reaktion auf ambiguit{\{}{\"{a}}{\}}t l{\{}{\"{a}}{\}}sst darauf schlie{\{}{\ss}{\}}en, dass leser/h{\{}{\"{o}}{\}}rer schnell erkennen, ob ein definites nomen nur einen referenten im diskursmodell hat oder nicht; (2) strukturell unterscheidet sich effekt ma{\{}{\ss}{\}}geblich von n400 und p600 {\textgreater} neuronale systeme f{\{}{\"{u}}{\}}r verarbeitung ambiger referenzen unterscheidt sich von denen semantischer und syntaktischer integration
- nref h{\{}{\"{a}}{\}}ngt nicht an schriftlicher oder auditiver wahrnehmung, sondern ist universell
- nref auch bei ambigen pronomen (van berkum et al. 2004)
- forschung zu textverstehen (bspw. kintsch 1998): comprehdners bauen situation model (tiefenstruktur/diskurs) und textbase (oberfl{\{}{\"{a}}{\}}chenverarbeitung)
- nieuwland et al. (2007): nref bezieht sich auf tiefenstruktur (experiment, in dem zun{\{}{\"{a}}{\}}chst zwei passende referenten eingef{\{}{\"{u}}{\}}hrt werden, einer jedoch sp{\{}{\"{a}}{\}}ter aus szene verschwindet)
- referenz wird sehr schnell auf einer tiefen ebene hergestellt
- nref bezieht sich auf prozess, der auf tiefenbene abl{\{}{\"{a}}{\}}uft
- erste m{\{}{\"{o}}{\}}glichkeit: nref neuronales korrelat davon, dass ambiguit{\{}{\"{a}}{\}}t bemerkt wird
- zweite m{\{}{\"{o}}{\}}glichkeit: zeigt kontrollierte verarbeitung an, wenn menschen diese ambiguit{\{}{\"{a}}{\}}t aufl{\{}{\"{o}}{\}}sen wollen
- dritte m{\{}{\"{o}}{\}}glichkeit: zeigt zus{\{}{\"{a}}{\}}tzliche neoronale aktivit{\{}{\"{a}}{\}}t an, die n{\{}{\"{o}}{\}}tig ist, um zwei referentielle interpretationen gleichzeitig im arbeitsged{\{}{\"{a}}{\}}chtnis zu halten
- wichtig f{\{}{\"{u}}{\}}r zuk{\{}{\"{u}}{\}}nfitge forschung: was zeigt nref wirklich an?
- unklar, ob nref nur referentielle schwierigkeiten anzeigt (eher: allgemein kontrollierte verarbeitung oder erh{\{}{\"{o}}{\}}hte belastung des working memory)
- bei pronomen ohne m{\{}{\"{o}}{\}}glichen referenten im kontext keine nref oder n400, sondern p600 {\textgreater} probanden nutzen nicht die m{\{}{\"{o}}{\}}glichkeit, pronomen als referenz auf bisher nicht eingef{\{}{\"{u}}{\}}hrte person zu werten, und suchen stattdessen in kontext nach passendem referenten
- zu wenige passende referenten zu haben, scheint ein anderes problem zu sein, als zu viele m{\{}{\"{o}}{\}}gliche referenten zu haben {\textgreater} nref k{\{}{\"{o}}{\}}nnte auf referentielle ambiguit{\{}{\"{a}}{\}}t beschr{\{}{\"{a}}{\}}nkt sein
