journal = {Lingua},
keywords = {Ellipsis,Sentence processing},
number = {PA},
pages = {78--95},
publisher = {Elsevier B.V.},
title = {{The psycholinguistics of ellipsis}},
url = {http://dx.doi.org/10.1016/j.lingua.2013.10.003},
volume = {151},
year = {2014}
}
