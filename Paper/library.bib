journal = {Discourse Process},
keywords = {agedifferences,goodenoughparsing,individualdifferences,parsing,sentenceprocessing},
number = {2},
pages = {205--238},
pmid = {17203135},
title = {{Younger and Older Adults' "Good-Enough" Interpretations of Garden-Path Sentences}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1761649/},
volume = {42},
year = {2006}
}
