journal = {Quarterly journal of experimental psychology},
keywords = {Individual differences.,Language production,Object naming,Sustained attention},
number = {4},
pages = {710--30},
pmid = {25214187},
title = {{Sustained attention in language production: an individual differences investigation.}},
url = {http://www.tandfonline.com/loi/pqje20 http://dx.doi.org/10.1080/17470218.2014.964736 http://www.ncbi.nlm.nih.gov/pubmed/25214187},
