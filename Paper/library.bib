journal = {Psychological Review},
keywords = {10,1037,Mathematical modeling,Response times,Visual working memory,a0034247,doi,dx,http,is the short-term memory,it,mathematical modeling,org,representations of stimulus inputs,response times,supp,supplemental materials,sys-,tem that maintains visual,visual working memory,wm},
