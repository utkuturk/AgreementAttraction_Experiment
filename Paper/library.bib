doi = {10.1037/0882-7974.21.3.448},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Rayner et al/2006/Rayner et al.{\_}2006{\_}The Effect of Word Frequency, Word Predictability, and Font Difficulty on the Eye Movements of Young and Older Reader.pdf:pdf},
