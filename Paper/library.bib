keywords = {Psychology,Quantitative Psychology,Social and Behavioral Sciences,bounded support,consistency,evidence,generalizability,induction,principle of parsimony},
publisher = {Computational Brain {\&} Behavior},
title = {{Limitations of Bayesian Leave-One-Out Cross-Validation for Model Selection}},
