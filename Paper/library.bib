issn = {0963-7214},
journal = {Current Directions in Psychological Science},
keywords = {an easy,and as a result,ex-gaussian analyses,individual differences,mean,observations,response time distributional analyses,summary statistic for multiple,the mean naturally provides,visual word recognition,we all love the},
number = {3},
pages = {160--166},
title = {{Moving Beyond the Mean in Studies of Mental Chronometry: The Power of Response Time Distributional Analyses}},
volume = {20},
year = {2011}
}
