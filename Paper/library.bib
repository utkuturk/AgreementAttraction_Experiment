keywords = {Adult,Cognition,Cognition: physiology,Female,Humans,Male,Models, Psychological,Proportional Hazards Models,Reaction Time,Reaction Time: physiology,Sleep Deprivation,Sleep Deprivation: physiopathology,Sleep Deprivation: psychology,Task Performance and Analysis,Young Adult},
