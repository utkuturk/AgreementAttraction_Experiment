journal = {Journal of Memory and Language},
keywords = {a key advantage of,available for analysis,be collected,during a trial is,eye-tracking,false positives,fixations that a participant,hypothesis testing,in practice,in theory,produces,reading,reading research is the,recorded and,some standard,statistics,the data that can,the entire sequence of,using eye-tracking methods in,wealth of},
pages = {1--18},
