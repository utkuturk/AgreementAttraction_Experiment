pages = {204--10},
pmid = {21327352},
title = {{Model discrimination through adaptive experimentation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3289091{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {18},
year = {2011}
