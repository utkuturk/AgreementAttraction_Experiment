keywords = {Adolescent,Adult,Eye Movements,Eye Movements: physiology,Humans,Middle Aged,Normal Distribution,Photic Stimulation,Reaction Time,Reaction Time: physiology,Reference Values,Visual Perception,Visual Perception: physiology,Young Adult},
