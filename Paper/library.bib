isbn = {0066-4308 978-0-8243-0259-7},
issn = {0066-4308},
journal = {Annual Review of Psychology},
keywords = {attention,delayed-response performance,encoding,frontal lobes,functional mri,medial temporal-lobe,posterior parietal cortex,predicts individual-differences,prefrontal cortex,proactive-interference,recognition memory,retrieval,storage,visual working-memory,working memory},
number = {August},
