keywords = {05 $\alpha$ level gives,against,arbitrariness,author some flex,formal models,however,ibility interpreting a,model selection,null hypothesis,null hypothesis significance testing,p value indicating evidence,unless converging evidence},
number = {1},
pages = {49--60},
publisher = {Hogrefe {\&} Huber Publishers},
title = {{Beyond the Null Ritual Formal Modeling of Psychological Processes}},
url = {http://libezproxy.open.ac.uk/login?url=http://search.ebscohost.com/login.aspx?direct=true{\&}db=pdh{\&}AN=zfp-217-1-49{\&}site=ehost-live{\&}scope=site},
volume = {217},
