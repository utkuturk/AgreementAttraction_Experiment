@article{EnochsonCulbertson:2015,
abstract = {Researchers in linguistics and related fields have recently begun exploiting online crowd-sourcing tools, like Amazon Mechanical Turk (AMT), to gather behavioral data. While this method has been successfully validated for various offline measures--grammaticality judgment or other forced-choice tasks--its use for mainstream psycholinguistic research remains limited. This is because psycholinguistic effects are often dependent on relatively small differences in response times, and there remains some doubt as to whether precise timing measurements can be gathered over the web. Here we show that three classic psycholinguistic effects can in fact be replicated using AMT in combination with open-source software for gathering response times client-side. Specifically, we find reliable effects of subject definiteness, filler-gap dependency processing, and agreement attraction in self-paced reading tasks using approximately the same numbers of participants and/or trials as similar laboratory studies. Our results suggest that psycholinguists can and should be taking advantage of AMT and similar online crowd-sourcing marketplaces as a fast, low-resource alternative to traditional laboratory research.},
author = {Enochson, Kelly and Culbertson, Jennifer},
doi = {10.1371/journal.pone.0116946},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Enochson, Culbertson/2015/Enochson, Culbertson{\_}2015{\_}Collecting psycholinguistic response time data using Amazon Mechanical Turk.pdf:pdf},
isbn = {10.1371/journal.pone.0116946},
issn = {19326203},
journal = {PLoS ONE},
number = {3},
pages = {1--17},
pmid = {25822348},
title = {{Collecting psycholinguistic response time data using Amazon Mechanical Turk}},
volume = {10},
year = {2015}
}
@inproceedings{Lim2004,
abstract = {Eye movement is a basic human behavior that offers a valuable means to$\backslash$nexplore human cognitive processes. This article introduces two modeling$\backslash$nstudies of eye movement. First, random menu search was modeled using a$\backslash$nqueueing network approach and second, a reinforcement learning algorithm$\backslash$nwas used to generate various eye movement patterns. Random menu search$\backslash$nis a task component involved in many human-machine interfaces and has$\backslash$nbeen modeled with several cognitive models including ACT-R and EPIC.$\backslash$nBased upon review of empirical data in menu search, and strengths and$\backslash$nlimitations of existing models, this article proposes a queueing network$\backslash$nmodel, which has been successfully applied in some other task domains$\backslash$n(e.g., response time, driver performance). The queueing network model$\backslash$nfor random menu search was implemented and evaluated through model$\backslash$nsimulation. In contrast to existing models that rely on four$\backslash$ntask-specific strategies to account for data, the queueing network model$\backslash$naccounted for the same data using only one strategy already employed in$\backslash$ncognitive modeling. To extend this parsimonious, ``minimal task$\backslash$nstrategy{\{}''{\}} modeling approach, Q-Learning, one of the reinforcement$\backslash$nlearning methods, was adopted to generate different patterns in eye$\backslash$nmovement. The same strategy from random menu search was used to generate$\backslash$neye movement, and the simulated eye movements were qualitatively$\backslash$ncompared to the human eye movement.},
