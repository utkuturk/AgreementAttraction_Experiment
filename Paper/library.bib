@article{Parker2017a,
abstract = {Many studies have shown that when forming a filler-gap dependency, comprehenders attempt to posit a gap site in advance of the input. However, it remains an open question what information they use to determine gap locations. The current study investigates parallelism in coordinate extraction structures, and asks whether comprehenders use parallelism constraints to structure their expectations about upcoming gap sites. Using a filled-gap paradigm, Experiments 1 and 2 show that comprehenders rely on parallelism to restrict the search for upcoming gap sites to specific locations in sentences with coordinate extraction. Experiment 3 shows that this effect cannot be reduced to processing factors, but may be due to a grammatically-based constraint on parallel extraction. Together, these results shed new light on the source and scope of active processing and parallelism effects in comprehension.},
author = {Parker, Dan},
doi = {10.1016/j.jml.2017.08.003},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Parker/2017/Parker{\_}2017{\_}Processing multiple gap dependencies Forewarned is forearmed.pdf:pdf},
isbn = {0749596X},
issn = {0749596X},
