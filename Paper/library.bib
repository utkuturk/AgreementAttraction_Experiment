journal = {Psychonomic bulletin {\&} review},
keywords = {Comprehension,Comprehension: physiology,Humans,Language,Memory,Memory: physiology,Models,Psychological,Short-Term,Short-Term: physiology},
number = {2},
pages = {243--68},
pmid = {23319178},
title = {{Memory mechanisms supporting syntactic comprehension.}},
volume = {20},
