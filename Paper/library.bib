journal = {Attention, perception {\&} psychophysics},
keywords = {bayesian modeling,response,visual search},
number = {3},
pages = {985--1010},
pmid = {25678272},
title = {{Modeling visual search using three-parameter probability functions in a hierarchical Bayesian framework.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25678272},
volume = {77},
year = {2015}
}
