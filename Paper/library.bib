issn = {0090-502X},
journal = {Memory {\{}{\&}{\}} Cognition},
keywords = {B{\_}scanpathsimilarity,Fixation,Humans,Ocular,Random Allocation,Reading,Saccades,article,empiricalstudy,eyetracking,gardenpath,reanalysis,regression},
number = {4},
pages = {551--561},
pmid = {12184556},
title = {{Overt reanalysis strategies and eye movements during the reading of mild garden path sentences}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12184556},
