file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Ono et al/2006/Ono et al.{\_}2006{\_}Real-Time Computation of Japanese Exclamatives and the Strength of Locality Biases in Sentence Comprehension.pdf:pdf},
journal = {Cognitive Studies},
keywords = {exclamatives,interrogatives,japanese,locality,processing exclamatives,reanalysis,running head,sentence processing,syntactic dependencies,typing mismatch effects},
number = {3},
pages = {1--26},
title = {{Real-Time Computation of Japanese Exclamatives and the Strength of Locality Biases in Sentence Comprehension}},
volume = {13},
