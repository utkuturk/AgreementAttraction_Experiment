keywords = {degradation,enced by manipulations of,ex-gaussian,eye movements,fixation durations,in order to understand,is influ-,it is important to,reading,recogni-,stimulus quality,the physical properties of,the processes underlying word,the visual,tion,understand how word recognition},
pages = {603--617},
