file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Heider, Dery, Roland/2014/Heider, Dery, Roland{\_}2014{\_}The processing of it object relative clauses Evidence against a fine-grained frequency account.pdf:pdf},
issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {Constraint-based approach,Corpus analysis,Discourse,Relative clause,Sentence processing},
pages = {58--76},
title = {{The processing of it object relative clauses: Evidence against a fine-grained frequency account}},
