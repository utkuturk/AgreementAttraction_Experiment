journal = {Journal of Sleep Research},
keywords = {Alertness,MSLT,Quantitative EEG,Subjective alertness,Waking EEG},
number = {3},
pages = {243--248},
pmid = {11012862},
title = {{Diurnal variations in the waking EEG: Comparisons with sleep latencies and subjective alertness}},
