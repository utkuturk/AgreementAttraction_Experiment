journal = {Proc Natl Acad Sci U S A},
keywords = {-23PAttention},
month = {jul},
number = {14},
pages = {4586--4590},
pmid = {6589612},
title = {{Function of the thalamic reticular complex: the searchlight hypothesis}},
volume = {81},
year = {1984}
