month = {jul},
number = {7},
pages = {1177--1187},
pmid = {16769618},
title = {{Recognition and attention guidance during contextual cueing in real-world scenes: evidence from eye movements}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed{\&}cmd=Retrieve{\&}dopt=AbstractPlus{\&}list{\_}uids=16769618},
