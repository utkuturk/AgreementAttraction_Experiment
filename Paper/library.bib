year = {1997}
}
@inproceedings{MalsburgKlieglVasishthCUNY2012,
abstract = {Scanpaths have played an important role in classic research on reading behavior. Nevertheless, they have largely been neglected in later research perhaps due to a lack of suitable analytical tools. Recently, von der Malsburg and Vasishth (2011) proposed a new measure for quantifying differ-ences between scanpaths and demonstrated that this measure can recover effects that were missed with the traditional eyetracking measures. However, the sentences used in that study were difficult to process and scanpath effects accordingly strong. The purpose of the present study was to test the validity, sensitivity, and scope of applicability of the scanpath measure, using simple sentences that are typically read from left to right. We derived predictions for the regularity of scanpaths from the literature on oculomotor control, sentence processing, and cognitive aging and tested these predic-tions using the scanpath measure and a large database of eye movements. All predictions were con-firmed: Sentences with short words and syntactically more difficult sentences elicited more irregular scanpaths. Also, older readers produced more irregular scanpaths than younger readers. In addition, we found an effect that was not reported earlier: Syntax had a smaller influence on the eye move-ments of older readers than on those of young readers. We discuss this interaction of syntactic pars-ing cost with age in terms of shifts in processing strategies and a decline of executive control as readers age. Overall, our results demonstrate the validity and sensitivity of the scanpath measure and thus establish it as a productive and versatile tool for reading research.},
address = {New York, NY, USA},
author = {von der Malsburg, Titus and Kliegl, Reinhold and Vasishth, Shravan and von der Malsburg, Titus and Kliegl, Reinhold and Vasishth, Shravan and von der Malsburg, Titus},
booktitle = {25th Annual CUNY Conference on Human Sentence Processing},
doi = {10.1111/cogs.12208},
editor = {Bradley, Dianne and Fern{\'{a}}ndez, Eva and Fodor, Janet Dean},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/von der Malsburg et al/2012/von der Malsburg et al.{\_}2012{\_}Determinants of scanpath regularity in reading.pdf:pdf},
isbn = {0364-0213},
issn = {1551-6709},
keywords = {Aging,Development,Eye movements,Individual differences,Language understanding,Oculo-motor control,Reading,Scanpaths,aging,oculo-motor control,reading,scanpaths,sentence processing},
number = {7},
pages = {82},
pmid = {25530253},
publisher = {CUNY Graduate School and University Center},
title = {{Determinants of scanpath regularity in reading}},
volume = {39},
year = {2012}
}
@unpublished{Bader:2011p701,
annote = {Main Claims: (1) case attaction can hapen even if the antecedent is morphologically not compatible with the attracted case (2) only marked case can be attracted, and only by unmarked antecedents =={\textgreater} structures computed by HSPM contain information (i) about synt. markedness - but (ii) not about morphological ambiguity of NPs According to Eberhard(1997) marked features [in his case, 'plural'] are specified by flags/features in the NP representation, whereas unmarked features have *no* representation, i.e. are default. According to him this does not lead to any problems with two unmarked features (Nom and Acc) because the difference between these is represented structurally. There are two possibilities for why Case Attraction occurs, CAH or PFH. CAH (Case Attraction Hypothesis): (Marked) Case features *errnously* percolate to the wrong NP, i.e. antecedent of the relative pronoun. PFH (Parallel Function Hypothesis): Parallelism (in case) is an ambiguity resolution strategy applied by the parser. [I don't understand how this would help parsing, so I find it hard to call it an "ambiguity resolution strategy". Also the errnous vs. non-errnous distinction is pretty subjective. The crucial difference, however, is the the requirement of ambiguity.] Experiment 1: CAH vs. PFH Stimuli: Wir alle konnten gut verstehen, ... SO: (a) control dass unsere Chefin ... (unambiguous) (b) ambiguous dass Judith, der man uebrigens das Auto geklaut hat, ... with RC (c) unambiguous dass unsere Chefin, der man uebrigens das Auto geklaut hat, ... with RC ... ein so gutes Angebot annahm OS: (d) control dass unserer Chefin ... (unambiguous) (e) ambiguous dass Judith, die uebrigens wieder umgezogen ist, ... with RC (f) unambiguous dass unserer Chefin, die uebrigens wieder umgezogen ist, ... with RC ... ein so gutes Angebot schmeichelte Predictions: CAH: Difficulty in (b,c) compared to (a) and (e) compared to (d). Case Attraction in conditions (b,c), because RelPro is marked, the antecedent is not. Additional garden path in (e), because of the local ambiguity of 'Judith' together with the generic SO preference in German (Judith will be analyzed as subject at first). PFH: Same as CAH, but no difficulty in (c), because it is not compatible with dative. Results: SO OS (a) (b) (c) (d) (e) (f) 92 77 74 85 85 85 - Accuracy 605 686 697 626 759 647 - Reaction Time Discussion: Bader sees PFH disconfirned, due to a difficuly in (c). [The SO preference predicted for (e) might be in RT instead of Accuracy, although it's a bit surprising.] [ However: The effects can be explained by complexity. In OS, even (Acc-initial) controls have a lower accuracy. Now, Dat-initial clauses might lead to even worse performance than Acc-initial ones. Thus, ]},
