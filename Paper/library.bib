doi = {10.1016/j.tree.2008.10.008},
eprint = {1003.3921v1},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Bolker et al/2009/Bolker et al.{\_}2009{\_}Generalized linear mixed models a practical guide for ecology and evolution.pdf:pdf},
