@article{SlioussarMalko:2016,
abstract = {3 4 Agreement attraction errors (such as the number error in the example " The key to the cabinets 5 are rusty ") have been extensively studied in the last twenty years. Two major types of explanation 6 have been suggested: i) attraction arises due to the properties of syntactic structure, because 7 the representation of the subject NP features is faulty or ambiguous; ii) attraction arises because 8 memory retrieval is noisy, and sometimes the wrong NP is selected during dependency resolution. 9 In this article we report the results of four experiments (one on production and three on compreh-10 ension) studying subject-verb gender agreement in Russian. Attraction effects were observed in 11 all of them, which can be more readily explained by the retrieval account (gender, unlike number, 12 is specified for each noun in the lexicon and hence is less likely to be erroneously assigned during 13 structure building). Previous research has shown that number agreement attraction is asymmetric: 14 plural provokes it, but singular does not. Our studies suggest that gender attraction is asymmetric 15 as well; however, the profiles are different in production and comprehension. Moreover, in the 16 comprehension experiments attraction was observed for all dependent noun genders, but only for 17 a subset of head noun genders. This goes against the traditional assumption that the features 18 of the dependent noun are crucial for attraction, showing the features of the head are more 19 important. We show that this approach can be extended to previous findings on attraction and 20 that there exists independent evidence for it.},
author = {Slioussar, Natalia and Malko, Anton},
doi = {10.3389/fpsyg.2016.01651},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Slioussar, Malko/2016/Slioussar, Malko{\_}2016{\_}Gender agreement attraction in Russian production and comprehension evidence.pdf:pdf},
issn = {1664-1078},
keywords = {Agreement,Russian,attraction,comprehension,gender,production},
number = {November},
pages = {1--20},
title = {{Gender agreement attraction in Russian: production and comprehension evidence}},
volume = {7},
year = {2016}
}
