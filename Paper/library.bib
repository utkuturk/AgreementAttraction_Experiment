journal = {Behavioral and Brain Sciences},
keywords = {cognition,humans,individuality,linguistics,memory,memory disorders,memory disorders psychology,models,neurological,psychological,short term,short term classification,verbal learning,verbal learning classification},
number = {1},
pages = {77--94; discussion 95--126},
