number = {0},
pages = {1--56},
publisher = {Taylor {\&} Francis},
title = {{Wait a second ! Delayed impact of argument roles on on-line verb prediction}},
url = {https://doi.org/10.1080/23273798.2018.1427878},
volume = {44},
year = {2017}
}
