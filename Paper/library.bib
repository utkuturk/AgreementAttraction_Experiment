journal = {Language and Cognitive Processes},
number = {5},
pages = {513--562},
title = {{Processing VP-ellipsis and VP- anaphora with structurally parallel and nonparallel antecedents: An eye-tracking study}},
volume = {18},
year = {2013}
}
