arxivId = {1901.06889},
author = {Schad, Daniel J},
eprint = {1901.06889},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Schad/2019/Schad{\_}2019{\_}The posterior probability of a null hypothesis given a 3 statistically significant result.pdf:pdf},
keywords = {Bayesian inference,Null hypothesis significance testing,statisti-cal power 10},
title = {{The posterior probability of a null hypothesis given a 3 statistically significant result}},
