journal = {Psychonomic Bulletin and Review},
keywords = {Bayes factor,Design analysis,Design planning,Power analysis,Sequential testing},
number = {1},
pages = {128--142},
pmid = {28251595},
title = {{Bayes factor design analysis: Planning for compelling evidence}},
volume = {25},
year = {2018}
}
