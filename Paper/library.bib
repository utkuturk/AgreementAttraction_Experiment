issn = {1939-1277},
journal = {Journal of experimental psychology. Human perception and performance},
keywords = {1922,1952,1966,2008,accuracy trade-off,capacity,decision making,forstmann et al,garret,hick,human perception and performance,is one of the,linear ballistic accumulator model,most pervasive effects in,oldest and,ollman,sat,speed,the speed},
number = {3},
pages = {1183--202},
pmid = {24635202},
title = {{Assessing the speed--accuracy trade-off effect on the capacity of information processing.}},
