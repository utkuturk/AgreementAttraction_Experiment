journal = {Brain and language},
keywords = {Adolescent,Adult,Attention,Attention: physiology,Brain Mapping,Electroencephalography,Evoked Potentials, Visual,Evoked Potentials, Visual: physiology,Female,Humans,Male,Semantics,Vocabulary},
