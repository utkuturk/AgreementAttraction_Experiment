journal = {Cognitive Science},
keywords = {Linguistic universals,Statistical methods,Statistical model,Typology},
number = {4},
pages = {736--756},
pmid = {24117660},
title = {{Quantitative standards for absolute linguistic universals}},
volume = {38},
