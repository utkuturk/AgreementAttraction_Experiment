journal = {Collabra},
keywords = {1,5,Bayes factors,Bayesian analysis,Inference,a crisis of confidence,bayes factors,bayesian analysis,by the recent,crisis is fueled by,empirical phe-,inference,model selection,nomena may not replicate,psychological science is facing,robustly,the suspicion that many,this},
