journal = {Frontiers in Human Neuroscience},
keywords = {Attention,Long Term Memory,Models,Neuroimaging,Neurosciences,Short Term Memory,focus of attention,long-term memory,memory models,representational states in memory,short-term memory,working memory},
number = {November},
