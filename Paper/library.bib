keywords = {Evidence accumulation,Lexical-decision task,Linear ballistic accumulator,Mathematical modeling,Response time},
number = {AUG},
pages = {292},
pmid = {22936920},
publisher = {Frontiers},
title = {{Linear deterministic accumulator models of simple choice}},
