file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Lucas/2012/Lucas{\_}2012{\_}Contact-induced grammatical change Towards an explicit account.pdf:pdf},
issn = {0176-4225},
journal = {Diachronica},
keywords = {borrowing,first language attrition,imposition,language acquisition,language contact,second,syntactic change},
