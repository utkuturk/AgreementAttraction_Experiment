@article{Budiu2004,
abstract = {We present interpretation-based processing-a theory of sentence processing that builds a syntactic and a semantic representation for a sentence and assigns an interpretation to the sentence as soon as possible. That interpretation can further participate in comprehension and in lexical processing and is vital for relating the sentence to the prior discourse. Our theory offers a unified account of the processing of literal sentences, metaphoric sentences, and sentences containing semantic illusions. It also explains how text can prime lexical access. We show that word literality is a matter of degree and that the speed and quality of comprehension depend both on how similar words are to their antecedents in the preceding text and how salient the sentence is with respect to the preceding text. Interpretation-based processing also reconciles superficially contradictory findings about the difference in processing times for metaphors and literals. The theory has been implemented in ACT-R [Anderson and Lebiere, The Atomic Components of Thought, Lawrence Erlbaum Associates Publishers, Mahwah, NJ, 1998]. {\textcopyright} 2003 Cognitive Science Society, Inc. All rights reserved.},
author = {Budiu, Raluca and Anderson, John R.},
doi = {10.1016/j.cogsci.2003.10.001},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Budiu, Anderson/2004/Budiu, Anderson{\_}2004{\_}Interpretation-based processing A unified theory of semantic sentence comprehension.pdf:pdf},
isbn = {0364-0213},
issn = {03640213},
journal = {Cognitive Science},
keywords = {ACT-R,Computational modeling,Language comprehension,Metaphor,Moses illusion,Semantics,Text priming},
number = {1},
pages = {1--44},
title = {{Interpretation-based processing: A unified theory of semantic sentence comprehension}},
volume = {28},
year = {2004}
}
