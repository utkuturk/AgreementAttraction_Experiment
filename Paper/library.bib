issn = {00100277},
journal = {Cognition},
keywords = {Coherence relations,Discourse processing,Implicit causality,Relative clause attachment ambiguity},
number = {3},
pages = {339--358},
pmid = {21216396},
publisher = {Elsevier B.V.},
title = {{Anticipating explanations in relative clause processing}},
