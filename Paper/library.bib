journal = {Journal of Memory and Language},
keywords = {Anticipation,Automaticity,N400,Post-N400 positivity,Prediction,Self-paced reading},
number = {April},
pages = {203--216},
title = {{Goals and strategies influence lexical prediction during sentence comprehension}},
url = {http://dx.doi.org/10.1016/j.jml.2016.10.002},
