isbn = {1664-1078},
issn = {1664-1078},
journal = {Frontiers in psychology},
keywords = {Mandarin Chinese,Reflexive processing,Speed-accuracy trade-off,mandarin chinese,reflexive processing,sentence,sentence processing,speed-accura,speed-accuracy trade-off,working memory},
number = {September},
pages = {1025},
pmid = {25309486},
title = {{The structure-sensitivity of memory access: evidence from Mandarin Chinese.}},
volume = {5},
year = {2014}
