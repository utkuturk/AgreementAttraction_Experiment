@article{Carrithers1984,
abstract = {Eye-movement patterns during reading are consistent with proposition-by-proposition models of speech comprehension. (1) Fixation times are least affected by word length at ends of clauses: this confirms the theory based on the study of speech comprehension that, at such points, attention is directed inward to text integration. (2) Fixation time for words at the end of clauses is longer for canonical sequences which conform to the pattern, "N-V-N = actor-action-object"; this confirms the view that complete semantic integration of such sequences is routinely delayed until their completion. A two-variable model based on principles of exhaustive visual search and speech comprehension accounts for 80{\%} of the fixation durations in a sample paragraph from Just and Carpenter (1980) and Thibadeau, just and Carpenter (1982). This comprehension-based model compares favorably with their models constructed within a production-system framework. This result clarifies the relationship between reading and listening. ?? 1997 Elsevier Science Ltd. All rights reserved.},
author = {Carrithers, Caroline and Bever, Thomas G.},
doi = {10.1016/S0364-0213(84)80015-4},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Carrithers, Bever/1984/Carrithers, Bever{\_}1984{\_}Eye-fixation patterns during reading confirm theories of language comprehension.pdf:pdf},
isbn = {0364-0213},
