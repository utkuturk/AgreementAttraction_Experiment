eprint = {1702.00556},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Vasishth et al/2018/Vasishth et al.{\_}2018{\_}The statistical significance filter leads to overoptimistic expectations of replicability.pdf:pdf},
isbn = {9780979806490},
issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {Bayesian data analysis,Expectation,Locality,Parameter estimation,Replicability,Surprisal,Type M error,bayesian data,locality,replicability,surprisal,type m error},
number = {August},
pages = {151--175},
publisher = {Elsevier},
title = {{The statistical significance filter leads to overoptimistic expectations of replicability}},
