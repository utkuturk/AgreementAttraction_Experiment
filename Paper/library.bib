@article{Hornig2005,
abstract = {We propose two principles that facilitate integration of two relational premises in spatial reasoning. Integration is easier if the anaphor in the second premise, P2, bears the role of the relatum (relatum = given). Moreover, integration is easier if, in P2, the anaphor is mentioned before the new element (given-new). In premises with canonical word order (grammatical subjects mentioned first), these principles always conflict with one another. In topicalized statements mentioning the prepositional phrase first, the two principles work in tandem. By varying word order, we tested the two principles by measuring P2 comprehension times. Comprehension times indicated that integration was easiest when P2 obeyed both principles and most difficult when both principles were violated. Canonical premises were of intermediate difficulty. This pattern emerged regardless of whether the anaphor was a definite description or a pronoun.},
author = {H{\"{o}}rnig, Robin and Oberauer, Klaus and Weidenfeld, Andrea},
doi = {10.3758/BF03195303},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/H{\"{o}}rnig, Oberauer, Weidenfeld/2005/H{\"{o}}rnig, Oberauer, Weidenfeld{\_}2005{\_}Two principles of premise integration in spatial reasoning.pdf:pdf},
