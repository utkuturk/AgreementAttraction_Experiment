issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {ACT-R,Agreement,Memory retrieval,Reflexives,Sentence processing},
number = {2},
pages = {85--103},
publisher = {Elsevier Inc.},
title = {{Contrasting intrusion profiles for agreement and anaphora: Experimental and modeling evidence}},
url = {http://dx.doi.org/10.1016/j.jml.2013.04.003},
volume = {69},
year = {2013}
