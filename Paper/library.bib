journal = {Vision research},
keywords = {Attention,Attention: physiology,Cues,Discrimination (Psychology),Discrimination (Psychology): physiology,Humans,Models,Photic Stimulation,Photic Stimulation: methods,Psychological,Reaction Time,Visual Fields,Visual Fields: physiology,Visual Perception,Visual Perception: physiology},
month = {jun},
