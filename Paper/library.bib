isbn = {0749-596X},
issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {Pronoun resolution,Speed-accuracy tradeoff,Syntactic clefting,Working memory},
number = {3},
pages = {357--383},
title = {{The role of prominence in pronoun resolution: Active versus passive representations}},
volume = {56},
year = {2007}
}
