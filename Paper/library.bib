issn = {0963-7214},
journal = {Current Directions in Psychological Science},
keywords = {analogy,cognitive modeling,computational modeling,debate about theories,however much we may,random walk model,reproducibility,science depends on reproducibility,scientific reasoning,scientists tacitly assume that,we are all},
