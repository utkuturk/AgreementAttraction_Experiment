doi = {10.1080/01690965.2013.866684},
file = {:C$\backslash$:/Users/Surface/Documents/Mendeley Desktop/Dillon, Clifton, Frazier/2013/Dillon, Clifton, Frazier{\_}2013{\_}Pushed aside Parentheticals, memory and processing.pdf:pdf},
issn = {23273801},
journal = {Language, Cognition and Neuroscience},
