month = {mar},
number = {3},
pages = {1126--1177},
pmid = {17662975},
publisher = {ELSEVIER SCIENCE BV},
title = {{Expectation-based syntactic comprehension}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17662975 http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed{\&}cmd=Retrieve{\&}dopt=AbstractPlus{\&}list{\_}uids=1142309711149801035related:Sx6QTKFM2g8J},
